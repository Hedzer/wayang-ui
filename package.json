{
  "version": "0.2.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --name WayangUI --format cjs,esm,umd --target node",
    "build": "tsdx build --name WayangUI --format cjs,esm,umd --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --name WayangUI --format cjs,esm,umd --target node",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "wayang-ui",
  "author": "Hedzer",
  "module": "dist/wayang-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/wayang-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wayang-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "husky": "^4.3.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "nexie": "^0.2.2"
  }
}
